    "format",
    "formatter"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.21.0",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-proposal-export-default-from": "~7.18.10",
    "@babel/polyfill": "~7.12.1",
    "@babel/preset-env": "~7.20.2",
    "@borgar/eslint-config": "~3.0.0",
    "babel-loader": "~9.1.2",
    "eslint": "~8.35.0",
    "eslint-plugin-import": "~2.27.5",
    "tap-min": "~2.0.0",
    "tape": "~5.6.3",
