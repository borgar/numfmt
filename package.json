{
  "name": "numfmt",
  "description": "Full Excel style number formatting",
  "author": "Borgar Ãžorsteinsson <borgar@borgar.net>",
  "version": "2.3.1",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "start": "nodemon -w test -w lib -x 'SKIPTABLES=1 tape 'test/*-test.js'|tap-min'",
    "lint": "eslint lib test rollup.config.js",
    "test-all": "tape 'test/*-test.js'",
    "test": "SKIPTABLES=1 tape 'test/*-test.js'|tap-min"
  },
  "main": "./dist/numfmt.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": "./dist/numfmt.js",
      "default": "./lib/index.js"
    }
  },
  "preferGlobal": false,
  "repository": "git://github.com/borgar/numfmt.git",
  "homepage": "https://github.com/borgar/numfmt",
  "bugs": {
    "url": "http://github.com/borgar/numfmt/issues"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "keywords": [
    "excel",
    "spreadsheet",
    "number",
    "format",
    "formatter"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.18.2",
    "@borgar/eslint-config": "~2.2.1",
    "eslint": "~8.17.0",
    "eslint-plugin-import": "~2.26.0",
    "tap-min": "~2.0.0",
    "tape": "~5.5.3"
  },
  "dependencies": {
    "@babel/preset-env": "~7.18.2",
    "@rollup/plugin-babel": "~5.3.1",
    "rollup": "~1.32.1",
    "rollup-plugin-uglify": "~6.0.4"
  }
}
